# This file is sourced during build

DEFAULT_LIBVIRT_VERSION="6.6.0-3"
DEFAULT_QEMU_VERSION="5.1.0-6"

IMAGE_NAME="${IMAGE_NAME:-kubevirt/libvirt}"

FEDORA_VERSION="${FEDORA_VERSION:-32}"
LIBVIRT_VERSION="${LIBVIRT_VERSION:-$DEFAULT_LIBVIRT_VERSION}"
QEMU_VERSION="${QEMU_VERSION:-$DEFAULT_QEMU_VERSION}"

TARGET_ARCHITECTURES="${TARGET_ARCHITECTURES:-amd64 arm64}"
